{
  "name": "server_instagram_clone",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "compile": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./proto --grpc_out=grpc_js:./proto -I ./proto ./proto/*.proto && grpc_tools_node_protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=grpc_js:./proto -I ./proto ./proto/*.proto",
    "build": "npx tsc",
    "start": "tsc && node dist/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\""
  },
  "author": "chico",
  "license": "ISC",
  "dependencies": {
    "@grpc/grpc-js": "^1.7.0",
    "@types/express": "^4.17.13",
    "@types/node": "^18.7.10",
    "body-parser": "^1.15.2",
    "concurrently": "^7.3.0",
    "consign": "^0.1.6",
    "cors": "^2.8.5",
    "express": "^4.14.0",
    "inversify": "^6.0.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^2.2.10",
    "mongoose": "^5.11.0",
    "nodemon": "^2.0.19",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.1.5",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mongoose": "^5.11.97",
    "grpc_tools_node_protoc_ts": "^5.3.2",
    "grpc-tools": "^1.11.2"
  }
}
